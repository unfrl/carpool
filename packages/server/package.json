{
    "name": "@carpool/server",
    "version": "0.0.0",
    "description": "description",
    "author": "Unfrl",
    "license": "MIT",
    "scripts": {
        "build": "tsc -p tsconfig.build.json",
        "start": "ts-node -r tsconfig-paths/register src/main.ts",
        "dev-prep": "docker stop -t 2 carpool_postgres carpool_adminer carpool_redis carpool_redis_commander ; docker-compose up -d postgres adminer redis redis_commander",
        "start:dev": "yarn run dev-prep ; nodemon",
        "start:debug": "yarn run dev-prep ; nodemon --config nodemon-debug.json",
        "prestart:prod": "rimraf dist && npm run build",
        "start:prod": "node dist/main.js",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "build:client": "autorest api-swagger.yaml"
    },
    "dependencies": {
        "@carpool/common": "^1.0.0",
        "@nest-modules/mailer": "^1.1.3",
        "@nestjs/common": "^6.8.3",
        "@nestjs/core": "^6.8.3",
        "@nestjs/jwt": "^6.1.1",
        "@nestjs/passport": "^6.1.0",
        "@nestjs/platform-express": "^6.8.3",
        "@nestjs/platform-socket.io": "^6.8.3",
        "@nestjs/swagger": "^3.1.0",
        "@nestjs/typeorm": "^6.2.0",
        "@nestjs/websockets": "^6.8.3",
        "bcrypt": "^3.0.7",
        "bull": "^3.12.1",
        "class-transformer": "^0.2.3",
        "class-validator": "^0.10.1",
        "crypto-random-string": "^3.0.1",
        "google-auth-library": "^5.5.1",
        "guid-typescript": "^1.0.9",
        "ioredis": "^4.14.1",
        "nest-bull": "^0.9.0",
        "nodemailer": "^6.3.1",
        "passport": "^0.4.0",
        "passport-jwt": "^4.0.0",
        "pg": "^7.8.2",
        "pg-hstore": "^2.3.2",
        "rate-limiter-flexible": "^1.1.6",
        "reflect-metadata": "^0.1.13",
        "rimraf": "^2.6.2",
        "rxjs": "^6.2.2",
        "shortid": "^2.2.15",
        "socket.io": "^2.3.0",
        "socket.io-redis": "^5.2.0",
        "swagger-ui-express": "^4.1.1",
        "typeorm": "^0.2.15"
    },
    "devDependencies": {
        "@nestjs/testing": "^6.8.0",
        "@types/bcrypt": "^3.0.0",
        "@types/bull": "^3.10.6",
        "@types/crypto-random-string": "^2.0.0",
        "@types/express": "^4.16.0",
        "@types/ioredis": "^4.0.18",
        "@types/jest": "^23.3.1",
        "@types/node": "^12.12.14",
        "@types/nodemailer": "^6.2.2",
        "@types/shortid": "^0.0.29",
        "@types/socket.io": "^2.1.4",
        "@types/socket.io-redis": "^1.0.25",
        "@types/supertest": "^2.0.5",
        "autorest": "^2.0.4283",
        "jest": "^23.5.0",
        "nodemon": "^1.18.3",
        "supertest": "^3.1.0",
        "ts-jest": "^23.1.3",
        "ts-loader": "^4.4.2",
        "ts-node": "^7.0.1",
        "tsconfig-paths": "^3.5.0",
        "typescript": "^3.7.2"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "test",
        "testRegex": ".test.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}